<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="HighlightToolbar">
        <attr name="highlightTitle" format="string|reference"/>
        <attr name="highlightSubTitle" format="string|reference"/>
        <attr name="icon" format="reference"/>
    </declare-styleable>

    <declare-styleable name="AspectRatioImageView">
        <attr name="aspectRatio" format="float"/>
    </declare-styleable>

    <declare-styleable name="ConsoleTextView">
        <attr name="animate" format="boolean"/>
        <attr name="duration" format="integer"/>
        <attr name="stayBottom" format="boolean"/>
    </declare-styleable>
</resources>